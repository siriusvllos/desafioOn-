{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "lib": [
      "es2019",
      "ES2022"
    ],
    "declaration": true,
    "outDir": "n8n/custom",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": [
    "n8n/custom/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
